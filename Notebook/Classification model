// Sentinel-2 Processing (NDVI Calculation)
/*var s2 = ee.ImageCollection("COPERNICUS/S2_SR_HARMONIZED")
            .filterBounds(Aoi)
            .filterDate('2020-05-01','2021-03-01')
            .filter(ee.Filter.lt("CLOUDY_PIXEL_PERCENTAGE", 10))
            .select(['B2', 'B3', 'B4', 'B8', 'B11', 'B12']); // Only bands needed for NDVI
var ndvi = s2.map(function(image) {
  return image.addBands(image.normalizedDifference(['B8', 'B4']).rename('NDVI')).clip(Aoi);
});

var ndvi_median = ndvi.mean().select('NDVI'); // Take median NDVI
print('NDVI Median:', ndvi_median);
var ndvi_osavi = s2.map(function(image) {
  var ndvi = image.normalizedDifference(['B8', 'B4']).rename('NDVI');
  var osavi = image.expression(
    '((NIR - RED) / (NIR + RED + 0.16)) * (1 + 0.16)', {
      'NIR': image.select('B8'),
      'RED': image.select('B4')
    }).rename('OSAVI');
  
  return image.addBands([ndvi, osavi]).clip(Aoi);
});
print('ndvi_osavi',ndvi_osavi)

