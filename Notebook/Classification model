var sugarcane = sugarcane.map(function(feat){
  feat = feat.set('class',sugarcane.get('class'))
  return feat
})

var maize = maize.map(function(feat){
  feat = feat.set('class',maize.get('class'))
  return feat
})
var sample = sugarcane.merge(maize).merge(urban);
print('Training Samples:', sample);
var insideAOI = sample.filterBounds(Aoi);
print('Samples inside AOI:', insideAOI);
var training = stackedImage.sampleRegions({
  collection: sample,   
  properties: ['class'], // Assign crop type
  scale: 10,
  tileScale: 2
});
print('Sampled Training Data:', training);


var split = 0.8;  
var trainingSample = training.randomColumn();  // Add a random column
var trainingData = trainingSample.filter(ee.Filter.lt('random', split));
var testingData = trainingSample.filter(ee.Filter.gte('random', split));
print('Training Data:', trainingData);
print('Testing Data:', testingData);
var classifier = ee.Classifier.smileRandomForest({
  numberOfTrees: 400, //400
  minLeafPopulation: 3,//3
  maxNodes: 250, //250
}).train({
  features: trainingData,
  classProperty: 'class',  // Make sure column name matches
  inputProperties: stackedImage.bandNames()
});
var classified = stackedImage.classify(classifier).clip(Aoi);
var trainPredictions = trainingData.classify(classifier);
var trainConfusionMatrix = trainPredictions.errorMatrix('class', 'classification');
print('Training Confusion Matrix:', trainConfusionMatrix);
print('Training Accuracy:', trainConfusionMatrix.accuracy());
var test = testingData.classify(classifier);
var confusionMatrix = test.errorMatrix('class', 'classification');
print('Testing Confusion Matrix:', confusionMatrix);
print('Testing Accuracy:', confusionMatrix.accuracy());
// Extracting the confusion matrix for testing
var confusionMatrix = test.errorMatrix('class', 'classification');
var classAccuracy = confusionMatrix.producersAccuracy();  // This gives class-specific accuracy
print('Class-Specific Accuracy (Producer\'s Accuracy):', classAccuracy);

// Calculate Kappa Coefficient
var kappa = confusionMatrix.kappa();
print('Kappa Coefficient:', kappa);

Map.addLayer(classified, { 
  min: 0, 
  max: 2, 
  palette: ['cyan', 'brown', 'green'] 
}, 'Classified Image');

var classCounts = training.aggregate_histogram('class');
print('Class Distribution:', classCounts);

Export.image.toDrive({
  image: classified,  // Your classified image
  description: 'classified_image',  // Name of the output file
  scale: 10,  // Resolution of the image (depends on the satellite data you are working with)
  region: Aoi,  // Define the area of interest (AOI) for which you want to export
  fileFormat: 'GeoTIFF',  // Choose file format, GeoTIFF is a common choice for images
  maxPixels: 1e8  // Maximum number of pixels to export, adjust based on image size
});
