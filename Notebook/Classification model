// Sentinel-1 Processing (VV & VH Polarization)
var dateRanges = [
  ['2020-05-01', '2020-05-15'],
  ['2020-05-16', '2020-05-31'],
  ['2020-06-01', '2020-06-15'],
  ['2020-06-16', '2020-06-30'],
  ['2020-07-01', '2020-07-15'],
  ['2020-07-16', '2020-07-31'],
  ['2020-08-01', '2020-08-15'],
  ['2020-08-16', '2020-08-31'],
  ['2020-09-01', '2020-09-15'],
  ['2020-09-16', '2020-09-30'],
  ['2020-10-01', '2020-10-15'],
  ['2020-10-16', '2020-10-31'],
  ['2020-11-01', '2020-11-15'],
  ['2020-11-16', '2020-11-30'],
  ['2020-12-01', '2020-12-15'],
  ['2020-12-16', '2020-12-31'],
  ['2021-01-01', '2021-01-15'],
  ['2021-01-16', '2021-01-31'],
  ['2021-02-01', '2021-02-15'],
  ['2021-02-16', '2021-02-27'],
  ['2021-03-01', '2021-03-15'],
  ['2021-03-16', '2021-03-27']
];
var getNDVI = function(start, end, index) {
  var collection = ee.ImageCollection('COPERNICUS/S2_SR')
    .filterDate(start, end)
    .filterBounds(Aoi)
    .filter(ee.Filter.lt('CLOUDY_PIXEL_PERCENTAGE', 20))
    .map(function(image) {
      var QA60 = image.select(['QA60']);
      var cloudMask = QA60.bitwiseAnd(1 << 10).eq(0);
      var cloudMaskedImage = image.updateMask(cloudMask);
      var ndvi = cloudMaskedImage.normalizedDifference(['B8', 'B4']).rename('NDVI'+(index+1));
      var osavi = cloudMaskedImage.expression(
        '((NIR - RED) / (NIR + RED + 0.16)) * (1 + 0.16)',{
          'NIR': cloudMaskedImage.select('B8'),
          'RED': cloudMaskedImage.select('B4')
        }).rename('OSAVI'+(index+1))
      return ndvi.addBands(osavi).copyProperties(cloudMaskedImage, cloudMaskedImage.propertyNames());
    });
    
  return collection.mean()
};

  
var getRvi = function(start, end, index) {
  var kernel = ee.Kernel.square(1);  // 3x3 kernel

  var collection = ee.ImageCollection('COPERNICUS/S1_GRD')
    .filterDate(start, end)
    .filterBounds(Aoi)
    .filter(ee.Filter.eq('instrumentMode', 'IW'))
    .filter(ee.Filter.listContains('transmitterReceiverPolarisation', 'VV'))
    .filter(ee.Filter.listContains('transmitterReceiverPolarisation', 'VH'))
    .filter(ee.Filter.eq('orbitProperties_pass', 'DESCENDING'))
    .map(function(img) {
      var vv = img.select('VV').reduceNeighborhood({
        reducer: ee.Reducer.mean(),
        kernel: kernel
      });

      var vh = img.select('VH').reduceNeighborhood({
        reducer: ee.Reducer.mean(),
        kernel: kernel
      });

      var rvi = vh.multiply(4).divide(vv.add(vh)).rename('RVI' + (index + 1));
      return rvi.copyProperties(img, img.propertyNames());
    });

  return collection.mean();  // return averaged RVI image
};

var combinedList = dateRanges.map(function(dates, index) {
  var ndvi = getNDVI(dates[0], dates[1], index);
  var Rvi = getRvi(dates[0], dates[1], index);
  return ndvi.addBands(Rvi);
});
// Convert to ee.Image and stack bands
var stackedImage = ee.Image(combinedList[0]);
for (var i = 1; i < combinedList.length; i++) {
  stackedImage = stackedImage.addBands(combinedList[i]);
}
stackedImage = stackedImage.clip(Aoi);
print("Stacked Time-Series Image (NDVI + OSAVI):", stackedImage);
/*var s1 = ee.ImageCollection("COPERNICUS/S1_GRD")
